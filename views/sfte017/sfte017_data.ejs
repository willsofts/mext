<form name="fslistform" id="fslistform" method="post" autocomplete="off">
	<input type="hidden" name="ajax" value="true"/>
	<input type="hidden" name="rowsPerPage" value="<%=page.setting?.rowsPerPage%>"/>
	<input type="hidden" name="limit" value="<%=page.setting?.limit%>"/>
	<input type="hidden" name="page" value="<%=page.setting?.page%>"/>
	<input type="hidden" name="userid" />
</form>
<form name="fssortform" id="fssortform" method="post" autocomplete="off">
	<input type="hidden" name="ajax" value="true"/>
	<input type="hidden" name="rowsPerPage" value="<%=page.setting?.rowsPerPage%>"/>
	<input type="hidden" name="limit" value="<%=page.setting?.limit%>"/>
	<input type="hidden" name="page" value="<%=page.setting?.page%>"/>
	<input type="hidden" name="orderDir" value="<%=page.invertOrder()%>"/>
	<input type="hidden" name="orderBy" />
</form>
<table id="datatable"
	class="table table-bordered table-hover table-striped tablesorter">
	<thead>
		<tr>
			<th class="text-center" style="cursor: default;"><label id="seqno_headerlabel"><%=label.get('seqno_headerlabel','No.')%></label></th>
			<th class="text-center"><a href="javascript:void(0)" class="alink-sorter" onclick="submitOrder(this,'username_surname')"><label id="username_surname_headerlabel"><%=label.get('username_surname_headerlabel','User Name')%></label></a></th>
			<th class="text-center"><a href="javascript:void(0)" class="alink-sorter" onclick="submitOrder(this,'email')"><label id="email_headerlabel"><%=label.get('email_headerlabel','Email')%></label></a></th>
			<th class="text-center"><a href="javascript:void(0)" class="alink-sorter" onclick="submitOrder(this,'issuer')"><label id="issuer_headerlabel"><%=label.get('issuer_headerlabel','Issuer')%></label></a></th>
			<th class="text-center"><a href="javascript:void(0)" class="alink-sorter" onclick="submitOrder(this,'createdatetime')"><label id="createdatetime_headerlabel"><%=label.get('createdatetime_headerlabel','Create Date')%></label></a></th>
			<th class="text-center"><a href="javascript:void(0)" class="alink-sorter" onclick="submitOrder(this,'confirmdatetime')"><label id="confirmdatetime_headerlabel"><%=label.get('confirmdatetime_headerlabel','Confirm Date')%></label></a></th>
			<th class="text-center"><a href="javascript:void(0)" class="alink-sorter" onclick="submitOrder(this,'factorflag')"><label id="factorflag_title_headerlabel"><%=label.get('factorflag_title_headerlabel','Factor')%></label></a></th>
			<th class="text-center" style="cursor: default;"><em class="fa fa-bolt" aria-hidden="true"></em></th>
		</tr>
	</thead>
	<tbody id="datatablebody">
		<%	let rows = data.dataset?.rows;
			if(rows && rows.length>0) { 
				for(let index=0; index < rows.length; index++) { 
					let item = rows[index]; %>
					<tr>
						<td class="text-center"><%=page.recordsNumber(index+1)%></td>
						<td align="left"><a href="javascript:void(0)" class="alink-data" onclick="submitViewhistory(this,'<%=item?.userid%>');"><%=item?.username%> &nbsp; <%=item?.usersurname%></a></td>
						<td align="left"><a href="javascript:void(0)" class="alink-data" onclick="submitViewhistory(this,'<%=item?.userid%>');"><%=item?.email%></a></td>
						<td align="left"><a href="javascript:void(0)" class="alink-data" onclick="submitViewhistory(this,'<%=item?.userid%>');"><%=item?.issuer%></a></td>
						<td align="center"><a href="javascript:void(0)" class="alink-data" onclick="submitViewhistory(this,'<%=item?.userid%>');"><%=page.format({value: item?.createdate, field:{type:"DATE"}})%> &nbsp; <%=page.format({value: item?.createtime, field:{type:"TIME"}})%></a></td>
						<td align="center"><a href="javascript:void(0)" class="alink-data" onclick="submitViewhistory(this,'<%=item?.userid%>');"><%=page.format({value: item?.confirmdate, field:{type:"DATE"}})%> &nbsp; <%=page.format({value: item?.confirmtime, field:{type:"TIME"}})%></a></td>
						<td align="center"><a href="javascript:void(0)" class="alink-data" onclick="submitViewhistory(this,'<%=item?.userid%>');"><%-page.serializeChecker(item?.factorflag)%></a></td>
						<td class="text-center">
							<button name="btn-view" type="button" class="btn-view" title="<%=label.get('btnedit_tooltip')%>" onclick="submitViewhistory(this,'<%=item?.userid%>');"></button>
						</td>						
					</tr>
				<%		}
			} else {
				if(rows) { %>
					<tr>
						<td class="text-center" colspan="8">
							<%=label.get('recordnotfound','Record not found')%>
						</td>
					</tr>
		<%		}
			}	%>	
	</tbody>
</table>
<div style="text-align: center;" class="fschaptertablelayer">
	<table style="margin: auto;" class="fschaptertable">
		<tr class="fschapterrow">
			<td class="fschaptercolumn">
				<form name="fschapterform" id="fschapterform" method="post" autocomplete="off">
					<input type="hidden" name="ajax" value="true"/>
					<input type="hidden" name="rowsPerPage" value="<%=page.setting?.rowsPerPage%>"/>
					<input type="hidden" name="limit" value="<%=page.setting?.limit%>"/>
					<input type="hidden" name="page" value="<%=page.setting?.page%>"/>
					<input type="hidden" name="orderDir" value="<%=page.setting?.orderDir%>"/>
					<input type="hidden" name="orderBy" value="<%=page.setting?.orderBy%>"/>
				</form>
				<div id="fschapterlayer">
					<%- page.createPaging() %>
				</div>
			</td>
		</tr>
	</table>
</div>
