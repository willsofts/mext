<%  let rows = data.dataset?.rows;
    if(rows && rows.length > 0) {
%>
<div id="page_first" class="pt-page">
	<div id="page_first_sub" class="panel-body pt-page-body" align="center">
		<div class="favor-navbox-tiles" style="width: 500px; height: 500px; ">
<%    
        for(let r of rows) {
            let iconfile = r["iconfile"];
            let programid = r["programid"];
            let url = r["url"];
            let shortname = r["shortname"];
            if(iconfile && iconfile!="") {
%>
<a href="javascript:void(0)" class="tile fa-box-title fav-app" pid="<%=programid%>" onclick="open_page('<%=programid%>','<%=url%>')" title="<%=programid%>"><div class="icon"><img class="fa fa-app-image" src="/img/apps/<%=iconfile%>" alt=""/></div><span class="title"><%=shortname%></span></a>		
<%                
            } else {
%>
<a href="javascript:void(0)" class="tile fa-box-title fav-app" pid="<%=programid%>" onclick="open_page('<%=programid%>','<%=url%>')" title="<%=programid%>"><div class="icon"><img class="fa fa-app-image" src="/img/apps/application.png" alt=""/></div><span class="title"><%=shortname%></span></a>		
<%
            }
        }
%>
        </div>
    </div>
</div>
<%        
    }
%>
