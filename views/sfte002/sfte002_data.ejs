<form name="fslistform" id="fslistform" method="post" autocomplete="off">
	<input type="hidden" name="ajax" value="true"/>
	<input type="hidden" name="rowsPerPage" value="<%=page.setting?.rowsPerPage%>"/>
	<input type="hidden" name="limit" value="<%=page.setting?.limit%>"/>
	<input type="hidden" name="page" value="<%=page.setting?.page%>"/>
	<input type="hidden" name="groupname" />
</form>
<form name="fssortform" id="fssortform" method="post" autocomplete="off">
	<input type="hidden" name="ajax" value="true"/>
	<input type="hidden" name="rowsPerPage" value="<%=page.setting?.rowsPerPage%>"/>
	<input type="hidden" name="limit" value="<%=page.setting?.limit%>"/>
	<input type="hidden" name="page" value="<%=page.setting?.page%>"/>
	<input type="hidden" name="orderDir" value="<%=page.invertOrder()%>"/>
	<input type="hidden" name="orderBy" />
</form>
<table id="datatable" class="table table-bordered table-hover table-striped tablesorter">
	<thead>
		<tr>
			<th width="75px"  class="text-center" style="cursor: default;"><label id="seqno_headerlabel"><%=label.get('seqno_headerlabel','No.')%></label></th>
			<th width="250px" class="text-center"><a href="javascript:void(0)" class="alink-sorter" onclick="submitOrder(this,'groupname')"><label id="groupname_headerlabel"><%=label.get('groupname_headerlabel','Group Name')%></label></a></th>
			<th class="text-center"><a href="javascript:void(0)" class="alink-sorter" onclick="submitOrder(this,'nameen')"><label id="nameen_headerlabel"><%=label.get('nameen_headerlabel','Name (English)')%></label></a></th>
			<th class="text-center"><a href="javascript:void(0)" class="alink-sorter" onclick="submitOrder(this,'nameth')"><label id="nameth_headerlabel"><%=label.get('nameth_headerlabel','Name (Thai)')%></label></a></th>
			<th class="text-center"><a href="javascript:void(0)" class="alink-sorter" onclick="submitOrder(this,'seqno')"><label id="sequenceno_headerlabel"><%=label.get('sequenceno_headerlabel','Sequence')%></label></a></th>
			<th width="100px" class="text-center" style="cursor: default;"><em class="fa fa-bolt" aria-hidden="true"></em></th>
		</tr>
	</thead>
	<tbody id="datatablebody">							
		<%	let rows = data.dataset?.rows;
			if(rows && rows.length>0) { 
				for(let index=0; index < rows.length; index++) { 
					let item = rows[index]; %>
						<tr>
							<td class="text-center"><%=page.recordsNumber(index+1)%></td>
							<td><a href="javascript:void(0)" class="alink-data" onclick="submitRetrieve(this,'<%=item?.groupname%>');"><%=item?.groupname%></a></td>
							<td><a href="javascript:void(0)" class="alink-data" onclick="submitRetrieve(this,'<%=item?.groupname%>');"><%=item?.nameen%></a></td>
							<td><a href="javascript:void(0)" class="alink-data" onclick="submitRetrieve(this,'<%=item?.groupname%>');"><%=item?.nameth%></a></td>
							<td class="text-center"><a href="javascript:void(0)" class="alink-data" onclick="submitRetrieve(this,'<%=item?.groupname%>');"><%=item?.seqno%></a></td>
							<td class="text-center">
									<a href="javascript:void(0)" onclick="submitEdit(this,'<%=item?.groupname%>');" class="btn-edit-record"><em class="fa fa-edit"></em></a>
									<button name="btn-edit" type="button" class="btn-edit" title="<%=label.get('btnedit_tooltip')%>"
											onclick="submitRetrieve(this,'<%=item?.groupname%>');"></button>
									<button type="button" class="btn-delete" title="<%=label.get('btndelete_tooltip')%>" 
											onclick="submitDelete(this,['<%=item?.groupname%>']);"></button>
							</td>
						</tr>
		<%		}
			} else {
				if(rows) { %>
					<tr>
						<td class="text-center" colspan="6">
							<%=label.get('recordnotfound','Record not found')%>
						</td>
					</tr>
		<%		}
			}	%>
	</tbody>
</table>	
<div class="fschaptertablelayer" style="text-align: center;">
<table class="fschaptertable" style="margin: auto;">
	<tr class="fschapterrow"><td class="fschaptercolumn">
	<form name="fschapterform" id="fschapterform" method="post" autocomplete="off">
		<input type="hidden" name="ajax" value="true"/>
		<input type="hidden" name="rowsPerPage" value="<%=page.setting?.rowsPerPage%>"/>
		<input type="hidden" name="limit" value="<%=page.setting?.limit%>"/>
		<input type="hidden" name="page" value="<%=page.setting?.page%>"/>
		<input type="hidden" name="orderDir" value="<%=page.setting?.orderDir%>"/>
		<input type="hidden" name="orderBy" value="<%=page.setting?.orderBy%>"/>
	</form>
	<div id="fschapterlayer">
		<%- page.createPaging() %>
	</div>
	</td>
	</tr>
</table>
</div>	
