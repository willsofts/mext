<form name="fslistform" id="fslistform" method="post" autocomplete="off">
	<input type="hidden" name="ajax" value="true"/>
	<input type="hidden" name="rowsPerPage" value="<%=page.setting?.rowsPerPage%>"/>
	<input type="hidden" name="limit" value="<%=page.setting?.limit%>"/>
	<input type="hidden" name="page" value="<%=page.setting?.page%>"/>
	<input type="hidden" name="reservenum" />
</form>
<form name="fssortform" id="fssortform" method="post" autocomplete="off">
	<input type="hidden" name="ajax" value="true"/>
	<input type="hidden" name="rowsPerPage" value="<%=page.setting?.rowsPerPage%>"/>
	<input type="hidden" name="limit" value="<%=page.setting?.limit%>"/>
	<input type="hidden" name="page" value="<%=page.setting?.page%>"/>
	<input type="hidden" name="orderDir" value="<%=page.invertOrder()%>"/>
	<input type="hidden" name="orderBy" />
</form>
<table id="datatable" class="table table-bordered table-hover table-striped tablesorter">
	<thead>
		<tr>
			<th class="text-center" style="cursor: default;"><label id="seqno_headerlabel"><%=label.get('seqno_headerlabel','No.')%></label></th>
			<th class="text-center"><a href="javascript:void(0)" class="alink-sorter" onclick="submitOrder(this,'reservenum')"><label id="reservenum_headerlabel"><%=label.get('reservenum_headerlabel','Reserve Alphabet/Number')%></label></a></th>
			<th class="text-center"><a href="javascript:void(0)" class="alink-sorter" onclick="submitOrder(this,'remarks')"><label id="remarks_headerlabel"><%=label.get('remarks_headerlabel','Remark')%></label></a></th>
			<th class="text-center" style="cursor: default;"><em class="fa fa-bolt" aria-hidden="true"></em></th>
		</tr>
	</thead>
	<tbody id="datatablebody">							
		<%	let rows = data.dataset?.rows;
			if(rows && rows.length>0) { 
				for(let index=0; index < rows.length; index++) { 
					let item = rows[index]; %>
					<tr>
						<td class="text-center"><%=page.recordsNumber(index+1)%></td>
						<td><a href="javascript:void(0)" class="alink-data" onclick="submitRetrieve(this,'<%=item?.reservenum%>');"><%=item?.reservenum%></a></td>
						<td><a href="javascript:void(0)" class="alink-data" onclick="submitRetrieve(this,'<%=item?.reservenum%>');"><%=item?.remarks%></a></td>
						<td class="text-center">
								<button name="btn-edit" type="button" class="btn-edit" title="<%=label.get('btnedit_tooltip')%>"
										onclick="submitRetrieve(this,'<%=item?.reservenum%>');"></button>
								<button type="button" class="btn-delete" title="<%=label.get('btndelete_tooltip')%>" 
										onclick="submitDelete(this,['<%=item?.reservenum%>']);"></button>
						</td>
					</tr>
		<%		}
			} else {
				if(rows) { %>
				<tr>
					<td class="text-center" colspan="4">
						<%=label.get('recordnotfound','Record not found')%>
					</td>
				</tr>
		<%		}
			}	%>
	</tbody>
</table>	
<div style="text-align: center;" class="fschaptertablelayer">
<table style="margin: auto;" class="fschaptertable">
	<tr class="fschapterrow"><td class="fschaptercolumn">
	<form name="fschapterform" id="fschapterform" method="post" autocomplete="off">
		<input type="hidden" name="ajax" value="true"/>
		<input type="hidden" name="rowsPerPage" value="<%=page.setting?.rowsPerPage%>"/>
		<input type="hidden" name="limit" value="<%=page.setting?.limit%>"/>
		<input type="hidden" name="page" value="<%=page.setting?.page%>"/>
		<input type="hidden" name="orderDir" value="<%=page.setting?.orderDir%>"/>
		<input type="hidden" name="orderBy" value="<%=page.setting?.orderBy%>"/>
	</form>
	<div id="fschapterlayer">
		<%- page.createPaging() %>
	</div>
	</td>
	</tr>
</table>
</div>
